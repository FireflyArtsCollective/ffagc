<ol class="breadcrumb">
  <li>Artists</li>
  <li class="active">Overview</li>
</ol>

<table class="table table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Contact</th>
    <th>Email</th>
  </tr>
  </thead>
  <tbody>
  <% @artists.each do |artist| %>
  <tr>
    <td><%= link_to artist.name, artist_path(artist) %></td>
    <td><%= artist.contact_name %></td>
    <td><%= artist.email %></td>
  </tr>
  <% end %>
  </tbody>
</table>
