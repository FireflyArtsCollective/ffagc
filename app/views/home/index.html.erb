<h1>Firefly Art Grants: <%= event_year %></h1>

<p>Are you an <%= link_to 'artist', new_sessions_artist_path %>?</p>

<p>Are you a member of the <%= link_to 'voting committee', new_sessions_voter_path %>?</p>

<p>Are you an <%= link_to 'administrator', new_sessions_admin_path %>?</p>

<% if false %>

    <hr/>

    <% if artist_logged_in? %>
        artist is logged in: <%= current_artist.name %> (<%= current_artist.email %>)
    <% else %>
        artist is NOT logged in
    <% end %>

    <br/> <br/>

    <% if voter_logged_in? %>
        voter is logged in: <%= current_voter.name %> (<%= current_voter.email %>)
    <% else %>
        voter is NOT logged in
    <% end %>

    <br/> <br/>

    <% if admin_logged_in? %>
        admin is logged in: <%= current_admin.name %> (<%= current_admin.email %>)
    <% else %>
        admin is NOT logged in
    <% end %>

<% end %>
